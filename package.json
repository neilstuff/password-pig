{
    "name": "password-pig",
    "version": "1.0.0",
    "description": "Password Pig - the password manager",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [{
                "target": "nsis",
                "arch": [
                    "x64",
                    "ia32"
                ]
            }]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/neilstuff/password-pig.git"
    },
    "keywords": [
        "Electron"
    ],
    "author": "Neil Brittliff",
    "license": "CC0-1.0",
    "dependencies": {
        "base64-js": "^1.3.1",
        "egoroof-blowfish": "^2.2.2",
        "electron-json-storage": "^4.1.1",
        "pug": "^3.0.2",
        "ejs": "^3.1.6",
        "mime": "^2.0.4",
        "hideshowpassword": "^2.2.0",
        "jquery": "^3.5.1",
        "uuid": "^8.0.0",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "jszip": "^3.7.1"
    },
    "devDependencies": {
        "electron": "^14.0.1",
        "electron-builder": "^22.11.7",
        "node": "^15.8.0"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/neilstuff/password-pig/issues"
    },
    "homepage": "https://github.com/thesourorange/neilstuff/password-pig#readme"
}