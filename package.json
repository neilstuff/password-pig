{
    "name": "password-pig",
    "version": "1.0.0",
    "description": "Password Pig",
    "main": "main.js",
    "author": "thesourorange",
    "license": "CC0-1.0",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
