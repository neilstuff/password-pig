    ],
    "dependencies": {
        "base64-js": "^1.3.1",
        "egoroof-blowfish": "^2.2.2",
        "electron-disable-file-drop": "^1.0.2",
        "electron-json-storage": "^4.1.1",
        "pug": "^3.0.2",
        "ejs": "^3.0.2",
        "mime": "^2.0.4",
        "hideshowpassword": "^2.2.0",
        "jquery": "^3.5.1",
        "uuid": "^8.0.0",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "jszip": "^3.6.0"
    },
    "devDependencies": {
        "electron": "^13.1.5",
        "electron-builder": "^22.5.1"
    }
